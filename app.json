{
  "expo": {
    "name": "denario",
    "slug": "denario",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/color_changed_logo.gif",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/color_changed_logo.gif",
      "resizeMode": "contain",
      "backgroundColor": "#0D0A19"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/color_changed_logo.gif",
        "backgroundColor": "#0D0A19"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/logo_color_change.png"
    },
    "extra": {
      "eas": {
        "projectId": "7b788120-a096-4b81-8ed1-593014185999"
      }
    },
    "owner": "crypto_wallet",
    "plugins": [
      "expo-barcode-scanner",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0D0A19",
          "image": "./assets/color_changed_logo.gif",
          "dark": {
            "image": "./assets/color_changed_logo.gif",
            "backgroundColor": "#0D0A19"
          },
          "imageWidth": 200
        }
      ]
    ]
  }
}
